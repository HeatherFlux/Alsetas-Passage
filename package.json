{
  "name": "alsetas-passage",
  "version": "1.1.0",
  "description": "A browser extension for integrating Pathbuilder2e with Discord",
  "scripts": {
    "test": "jest --config jest.config.cjs --detectOpenHandles",
    "test:watch": "jest --config jest.config.cjs --watch",
    "test:coverage": "jest --config jest.config.cjs --coverage",
    "build:chrome": "cd chrome && ./build.sh",
    "build:firefox": "cd firefox && ./build.sh",
    "build": "./build.sh"
  },
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-class-properties": "^7.22.5",
    "@babel/plugin-transform-modules-commonjs": "^7.22.15",
    "@babel/plugin-transform-private-methods": "^7.22.5",
    "@babel/plugin-transform-runtime": "^7.22.15",
    "@babel/preset-env": "^7.22.20",
    "@babel/runtime": "^7.22.15",
    "babel-jest": "^29.7.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-webextension-mock": "^3.8.9",
    "jsdom": "^22.1.0",
    "webpack": "^5.97.1",
    "webpack-cli": "^6.0.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yourusername/alsetas-passage.git"
  },
  "keywords": [
    "pathfinder",
    "pathbuilder",
    "discord",
    "browser-extension"
  ],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/yourusername/alsetas-passage/issues"
  },
  "homepage": "https://github.com/yourusername/alsetas-passage#readme"
}
